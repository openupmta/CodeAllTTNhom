
@{
    ViewData["Title"] = "Dashboardv1";
    Layout = "~/Views/Shared/_Adminlte.cshtml";
}



@section Styles{

    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/adminlte/components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/adminlte/components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/adminlte/components/Ionicons/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/adminlte/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/adminlte/dist/css/skins/_all-skins.min.css">
    <!-- Morris chart -->
    <link rel="stylesheet" href="~/Content/adminlte/components/morris.js/morris.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="~/Content/adminlte/components/jvectormap/jquery-jvectormap.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="~/Content/adminlte/components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Content/adminlte/components/bootstrap-daterangepicker/daterangepicker.css">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/Content/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

    <!-- DataTables -->s
    <link rel="stylesheet" href="~/Content/adminlte/components/datatables.net-bs/css/dataTables.bootstrap.min.css">
    <link rel="stylesheet" href="~/Areas/TTNhom_QLTTHPT/Asset/index.css" />

    <link rel="stylesheet" href="~/Areas/TTNhom_QLTTHPT/Asset/dashboard.css" />
}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            <b>
                Dashboard
                <small>Thống kê</small>
            </b>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-lg-3 col-xs-6" style="width:100%">
                <!-- small box -->
                <div class="small-box bg-red">
                    <div class="inner">
                        <h3>Quản Lý Nhân Sự</h3>
                        <table cellpadding="0" cellspacing="0" style="width:60%">
                            <col style="width:40%">
                            <col style="width:36%">
                            <col style="width:24%">
                            <tr>
                                <td><h4>Nhân sự hoạt động&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalStaffON</b></h4></td>
                                <td><h4>Nhân sự không hoạt động&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalStaffOFF</b></h4></td>
                            </tr>
                            <tr>
                                <td><h4>Quản lý&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalQL</b></h4></td>
                                <td><h4>Người dùng&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalND</b></h4></td>
                            </tr>

                        </table>
                    </div>
                    <div class="icon">
                        <i class="fa fa-user-alt"></i>
                    </div>
                    <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6" style="width:100%">
                <!-- small box -->
                <div class="small-box bg-yellow">
                    <div class="inner">
                        <h3>Quản Lý Thư Viện</h3>

                        <table cellpadding="0" cellspacing="0" style="width:60%">
                            <col style="width:40%">
                            <col style="width:36%">
                            <col style="width:24%">
                            <tr>
                                <td><h4>Số đầu sách&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalBook</b></h4></td>
                                <td><h4>Số tác giả&nbsp;&nbsp;: <b>@ViewBag.TotalAuthor</b></h4></td>
                                <td><h4>Số lượt mượn&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalBG</b></h4></td>
                            </tr>
                            <tr>
                                <td><h4>Số thể loại&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.TotalCategory</b></h4></td>
                                <td><h4>Số độc giả&nbsp;: <b>@ViewBag.TotalReader</b></h4></td>
                                <td><h4>Số lượt đã trả&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.AlreadyBG</b></h4></td>
                            </tr>
                            <tr>
                                <td><h4>Số nhà xuất bản&nbsp;: <b>@ViewBag.TotalPublishingCompany</b></h4></td>
                                <td><h4><b></b></h4></td>
                                <td><h4>Số lượt chưa trả&nbsp;: <b>@ViewBag.YetBG</b></h4></td>
                            </tr>

                        </table>
                    </div>
                    <div class="icon">
                        <i class="fa fa-book"></i>
                    </div>
                    <a href="/TTNhom_QLThuVien/Home/Index" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6" style="width:100%">
                <!-- small box -->
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3>Quản Lý Khách Sạn</h3>


                        <table cellpadding="0" cellspacing="0" style="width:60%">
                            <col style="width:40%">
                            <col style="width:36%">
                            <col style="width:24%">
                            <tr>
                                <td><h4>Số lượng nhân viên: <b>@ViewBag.NhanVien</b></h4></td>
                                <td><h4>Thu ngày: <b>@ViewBag.ThuNgay</b></h4></td>
                                <td><h4>Thu tháng: <b>@ViewBag.ThuThang</b></h4></td>
                            </tr>
                            <tr>
                                <td><h4>Số lượng khách hàng: <b>@ViewBag.KhachHang</b></h4></td>
                                <td><h4>Chi ngày: <b>@ViewBag.ChiNgay</b></h4></td>
                                <td><h4>Chi tháng: <b>@ViewBag.ChiThang</b></h4></td>
                            </tr>
                            <tr>
                                <td><h4><b></b></h4></td>
                                <td><h4><b></b></h4></td>
                                <td><h4><b></b></h4></td>
                            </tr>

                        </table>


                    </div>
                    <div class="icon">
                        <i class="fa fa-hotel"></i>
                    </div>
                    <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6" style="width:100%">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3>Quản Lý Kho</h3>
                        <table cellpadding="0" cellspacing="0" style="width:60%">
                            <col style="width:40%">
                            <col style="width:36%">
                            <col style="width:24%">
                            <tr>
                                <td><h4>Số Hàng Hóa&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.HH</b></h4></td>
                                <td><h4>Số Khách Hàng&nbsp;&nbsp;: <b>@ViewBag.KH</b></h4></td>

                            </tr>
                            <tr>
                                <td><h4>Số Nhà Cung Cấp&nbsp;: <b>@ViewBag.NCC</b></h4></td>
                                <td><h4>Số Phiếu Nhập&nbsp;&nbsp;&nbsp;: <b>@ViewBag.PN</b></h4></td>

                            </tr>
                            <tr>
                                <td><h4>Số Phiếu Xuất&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;: <b>@ViewBag.PX</b></h4></td>

                            </tr>

                        </table>
                    </div>
                    <div class="icon">
                        <i class="fa fa-key"></i>
                    </div>
                    <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-3 col-xs-6" style="width:100%">
                <!-- small box -->
                <div class="small-box bg-purple">
                    <div class="inner">
                        <h3>Quản Lý Trường THPT</h3>

                        <ul class="thongke">
                            <li class="col-lg-3 col-xs-6">Số Học Sinh : @ViewBag.totalStudent</li>
                            <li class="col-lg-3 col-xs-6">Số Học Sinh Xuất Sắc : @ViewBag.totalStudentXS</li>
                            <li class="col-lg-3 col-xs-6">Số Học Sinh Giỏi : @ViewBag.totalStudentG</li>
                        </ul>
                        <ul class="thongke">
                            <li class="col-lg-3 col-xs-6">Số Học Sinh Khá : @ViewBag.totalStudentK</li>
                            <li class="col-lg-3 col-xs-6">Số Học Sinh TB : @ViewBag.totalStudentTB</li>
                            <li class="col-lg-3 col-xs-6">Số Học Sinh Yếu : @ViewBag.totalStudentY</li>
                        </ul>
                        <ul class="thongke">
                            <li class="col-lg-3 col-xs-6">Số Học Sinh Diện Ưu Tiên: @ViewBag.totalStudent</li>
                            <li class="col-lg-3 col-xs-6">Số Giáo Viên : @ViewBag.totalTeacher</li>
                            <li class="col-lg-3 col-xs-6">Số Lớp : @ViewBag.totalClass</li>
                        </ul>


                        <div class="nav-tabs-custom" style="margin-top : 50px">
                            <!-- Tabs within a box -->
                            <ul class="nav nav-tabs pull-right">

                                <li class="pull-left header"><i class="fa fa-inbox"></i> Thống kê đánh giá học lực học sinh </li>
                            </ul>
                            <div class="tab-content no-padding">
                                <div class="box-header">
                                    @*<h3 class="box-title">Danh sách nhóm quyền</h3>*@
                                    <div class="row-button col-xs-4">

                                        <a href="@Url.Action("ExportToExcel","Dashboard")" class="btn btn-success" id="export"><i class="fas fa-file-excel"></i> Export To Excel</a>

                                    </div>
                                    <!-- /.box-header -->
                                    <div class="box-body">
                                        <table id="example1" class="table table-bordered table-striped">
                                            <thead>
                                                <tr>
                                                    <th>Học Sinh</th>
                                                    <th>Điểm TBHK</th>
                                                    <th>Điểm TK</th>
                                                    <th>Học Kỳ</th>
                                                    <th>Hạnh Kiểm</th>
                                                    <th>Năm Học</th>
                                                    <th>Học Lực</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @foreach (var item in ViewBag.HocSinh)
                                                {
                                                    <tr>
                                                        <td>@item.HOCSINH.HoTenHS</td>
                                                        <td>@item.DiemTBHK</td>
                                                        <td>@item.DiemTK</td>
                                                        <td>@item.HocKy</td>
                                                        <td>@item.HanhKiem</td>
                                                        <td>@item.NamHoc</td>
                                                        <td>@item.HocLuc</td>
                                                    </tr>
                                                }
                                            </tbody>
                                            <tfoot>
                                                <tr>
                                                    <th>Học Sinh</th>
                                                    <th>Điểm TBHK</th>
                                                    <th>Điểm TK</th>
                                                    <th>Học Kỳ</th>
                                                    <th>Hạnh Kiểm</th>
                                                    <th>Năm Học</th>
                                                    <th>Học Lực</th>
                                                </tr>
                                            </tfoot>
                                        </table>
                                    </div>
                                    <!-- /.box-body -->
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="icon">
                        <i class="fa fa-school"></i>
                    </div>
                    <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>

@section Scripts{

    <!-- jQuery 3 -->
    <script src="~/Content/adminlte/components/jquery/dist/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/adminlte/components/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>$.widget.bridge('uibutton', $.ui.button);</script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/adminlte/components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="~/Content/adminlte/components/raphael/raphael.min.js"></script>
    <script src="~/Content/adminlte/components/morris.js/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Content/adminlte/components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Content/adminlte/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/adminlte/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/adminlte/components/jquery-knob/dist/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Content/adminlte/components/moment/min/moment.min.js"></script>
    <script src="~/Content/adminlte/components/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/Content/adminlte/components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/Content/adminlte/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Content/adminlte/components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/adminlte/components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/adminlte/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Content/adminlte/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/adminlte/dist/js/demo.js"></script>

    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/adminlte/components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- DataTables -->
    <script src="~/Areas/TTNhom_QLTTHPT/Asset/scripts.js"></script>
    <script src="~/Content/adminlte/components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <!-- page script -->
    <script>
        $(function () {
            $('#example1').DataTable()
            $('#example2').DataTable({
                'paging': true,
                'lengthChange': false,
                'searching': false,
                'ordering': true,
                'info': true,
                'autoWidth': false
            })
        })</script>


}
